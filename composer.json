{
    "name": "proger0014/comparison-manager",
    "description": "Тестовое на тему менеджера автосопоставления адресов",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4.0",
        "yiisoft/yii2": "~2.0.45",
        "yiisoft/yii2-bootstrap5": "~2.0.2",
        "kartik-v/yii2-widget-select2": "dev-master",
        "kartik-v/yii2-icons": "dev-master",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "phpunit/phpunit": "^9.6",
        "testcontainers/testcontainers": "^1.0",
        "symfony/http-client": "^7.3",
        "php-webdriver/webdriver": "^1.15"
    },
    "autoload": {
        "psr-4": {
          "ComparisonManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "ComparisonManager\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "php-http/discovery": true
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
